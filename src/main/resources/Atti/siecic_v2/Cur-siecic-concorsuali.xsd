<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns="http://schemi.processotelematico.giustizia.it/siecic/concorsuali/curatore/v2" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:pt="http://schemi.processotelematico.giustizia.it/tipi/atti/v1" xmlns:at="http://schemi.processotelematico.giustizia.it/tipi/anagrafiche" xmlns:bt="http://schemi.processotelematico.giustizia.it/tipi" xmlns:evt="http://schemi.processotelematico.giustizia.it/siecic/eventi" xmlns:st="http://schemi.processotelematico.giustizia.it/siecic/tipibase" xmlns:tns="http://schemi.processotelematico.giustizia.it/siecic/concorsuali/curatore/v2" targetNamespace="http://schemi.processotelematico.giustizia.it/siecic/concorsuali/curatore/v2" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:import namespace="http://schemi.processotelematico.giustizia.it/tipi/atti/v1" schemaLocation="../base_v1/tipi-atti.xsd"/>
	<xs:import namespace="http://schemi.processotelematico.giustizia.it/tipi/anagrafiche" schemaLocation="../tipi-anagrafiche.xsd"/>
	<xs:import namespace="http://schemi.processotelematico.giustizia.it/siecic/tipibase" schemaLocation="../tipi-base-siecic.xsd"/>
	<xs:import namespace="http://schemi.processotelematico.giustizia.it/siecic/eventi" schemaLocation="../eventi.xsd"/>
	<xs:import namespace="http://schemi.processotelematico.giustizia.it/tipi" schemaLocation="../tipi-base.xsd"/>
	<xs:include schemaLocation="../questionario-evoluto/Questionario.xsd"/>
	<xs:annotation>
		<xs:documentation>ATTI DEL CURATORE</xs:documentation>
		<xs:documentation>Gli atti qui definiti si riferiscono a procedimenti in essere</xs:documentation>
	</xs:annotation>
	<xs:attributeGroup name="CoordinateBancarie">
		<xs:annotation>
			<xs:documentation>Relazione Periodica: Coordinate bancarie</xs:documentation>
		</xs:annotation>
		<xs:attribute name="ABI" type="xs:string" use="required"/>
		<xs:attribute name="CAB" type="xs:string" use="required"/>
		<xs:attribute name="CIN" type="xs:string" use="required"/>
		<xs:attribute name="CC" type="xs:string" use="required"/>
	</xs:attributeGroup>
	<xs:attributeGroup name="PersonaFisica">
		<xs:annotation>
			<xs:documentation>Relazione Periodica: Dati della persona fisica</xs:documentation>
		</xs:annotation>
		<xs:attribute name="Cognome" type="xs:string" use="required"/>
		<xs:attribute name="Nome" type="xs:string" use="required"/>
		<xs:attribute name="CodiceFiscale" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:length value="16"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="NatoA" type="xs:string" use="required"/>
		<xs:attribute name="DataNascita" type="xs:date" use="required"/>
		<xs:attributeGroup ref="IndirizzoCompleto"/>
	</xs:attributeGroup>
	<xs:attributeGroup name="PersonaGiuridica">
		<xs:annotation>
			<xs:documentation>Relazione Periodica: Dati della persona giuridica</xs:documentation>
		</xs:annotation>
		<xs:attribute name="Denominazione" type="xs:string" use="required"/>
		<xs:attribute name="FormaGiuridica" type="xs:string" use="required"/>
		<xs:attribute name="CodiceFiscale" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:length value="11"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attributeGroup ref="IndirizzoCompleto"/>
	</xs:attributeGroup>
	<xs:attributeGroup name="IndirizzoCompleto">
		<xs:annotation>
			<xs:documentation>Relazione Periodica: Dati di indirizzo</xs:documentation>
		</xs:annotation>
		<xs:attribute name="Indirizzo" type="xs:string" use="required"/>
		<xs:attribute name="CAP" type="xs:string" use="required"/>
		<xs:attribute name="Localita" type="xs:string" use="required"/>
		<xs:attribute name="Provincia" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:length value="2"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:attributeGroup>
	<xs:attributeGroup name="TipologieAttivo">
		<xs:annotation>
			<xs:documentation>Relazione Periodica:  Tipologie di attivo</xs:documentation>
		</xs:annotation>
		<xs:attribute name="Stimato" type="xs:float" use="optional"/>
		<xs:attribute name="DaRealizzare" type="xs:float" use="required"/>
		<xs:attribute name="Realizzato" type="xs:float" use="required"/>
	</xs:attributeGroup>
	<xs:attributeGroup name="RG">
		<xs:annotation>
			<xs:documentation>Relazione Periodica: Identificazione Procedura</xs:documentation>
		</xs:annotation>
		<xs:attribute name="Tribunale" type="xs:string" use="required"/>
		<xs:attribute name="Sezione" type="xs:string" use="required"/>
		<xs:attribute name="RuoloGenerale" type="xs:positiveInteger" use="required"/>
		<xs:attribute name="Sub" type="xs:positiveInteger" use="optional"/>
		<xs:attribute name="Anno" type="xs:gYear" use="required"/>
	</xs:attributeGroup>
	<xs:attributeGroup name="InformazioniCause">
		<xs:annotation>
			<xs:documentation>Relazione Periodica: Informazioni cause in corso</xs:documentation>
		</xs:annotation>
		<xs:attribute name="Oggetto" type="xs:string" use="required"/>
		<xs:attribute name="DataUdienza" type="xs:date" use="required"/>
		<xs:attribute name="Fase" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="Introduttiva"/>
					<xs:enumeration value="Prove"/>
					<xs:enumeration value="Decisione"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="Valore" type="xs:float" use="required"/>
	</xs:attributeGroup>
	<xs:complexType name="RiferimentoCausa">
		<xs:annotation>
			<xs:documentation>Relazione periodica: Riferimento ad una causa</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="numero" type="xs:nonNegativeInteger"/>
			<xs:element name="anno" type="xs:nonNegativeInteger"/>
		</xs:sequence>
		<xs:attribute name="ufficioGiudiziario" type="xs:string" use="optional"/>
		<xs:attribute name="ufficioSede" type="xs:string" use="optional"/>
	</xs:complexType>
	<xs:simpleType name="CategoriaEconomica">
		<xs:restriction base="xs:string">
			<xs:enumeration value="verso soci per finanziamenti"/>
			<xs:enumeration value="verso banche"/>
			<xs:enumeration value="verso altri finanziatori"/>
			<xs:enumeration value="verso fornitori"/>
			<xs:enumeration value="di cui verso imprese collegate e controllate"/>
			<xs:enumeration value="verso lavoratori subordinati e para-subordinati"/>
			<xs:enumeration value="verso professionisti"/>
			<xs:enumeration value="verso obbligazionisti"/>
			<xs:enumeration value="debiti tributari, verso istituti di previdenza e di sicurezza sociale"/>
			<xs:enumeration value="di cui per sanzioni"/>
			<xs:enumeration value="altri debiti"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="StatoLotto">
		<xs:restriction base="xs:string">
			<xs:enumeration value="non in vendita"/>
			<xs:enumeration value="aggiudicato"/>
			<xs:enumeration value="trasferito"/>
			<xs:enumeration value="abbandonato"/>
			<xs:enumeration value="in vendita"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="StatoUtilizzo">
		<xs:restriction base="xs:string">
			<xs:enumeration value="libero"/>
			<xs:enumeration value="detenuto con titolo"/>
			<xs:enumeration value="detenuto senza titolo"/>
			<xs:enumeration value="detenuto dal fallito"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="TipologiaCrediti">
		<xs:restriction base="xs:string">
			<xs:enumeration value="crediti verso clienti"/>
			<xs:enumeration value="crediti verso societa"/>
			<xs:enumeration value="crediti verso l'erario"/>
			<xs:enumeration value="crediti da azioni revocatorie"/>
			<xs:enumeration value="altri crediti"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="AnagraficaCuratore">
		<xs:annotation>
			<xs:documentation>Elemento di anagrafica curatore per relazione periodica. Se specificati più curatori si intende un incarico collegiale.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Curatore" maxOccurs="unbounded">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Nome" type="xs:string"/>
							<xs:element name="Cognome" type="xs:string"/>
							<xs:element name="CodiceFiscale" type="bt:CodiceFiscale"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="elementoRendiconto">
		<xs:annotation>
			<xs:documentation>Elemento di rendiconto con riferimenti alle voci di contabilità del fallimento</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="st:importo"/>
			<xs:element name="PDC" type="st:VociPDC" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="LottoMobiliare">
		<xs:sequence>
			<xs:element name="Descrizione" type="xs:string"/>
			<xs:element name="ValoreStima" type="bt:Valuta">
				<xs:annotation>
					<xs:documentation>Valorizzare obbligatoriamente per lotto in stato aggiudicato e trasferito, altrimenti specificare motivazione nel campo Note</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ValoreContabile" type="bt:Valuta">
				<xs:annotation>
					<xs:documentation>Valorizzare obbligatoriamente per lotto in stato aggiudicato e trasferito, altrimenti specificare motivazione nel campo Note</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ValoreAggiudicazione" type="bt:Valuta">
				<xs:annotation>
					<xs:documentation>Valorizzare obbligatoriamente per lotto in stato aggiudicato e trasferito, altrimenti specificare motivazione nel campo Note</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IncaricatoVendita">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Nome" type="xs:string"/>
						<xs:element name="Cognome" type="xs:string"/>
						<xs:element name="CodiceFiscale" type="bt:CodiceFiscale"/>
						<xs:element name="Qualifica">
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="curatore"/>
									<xs:enumeration value="professionista delegato"/>
									<xs:enumeration value="Istituto Vendite Giudiziarie"/>
									<xs:enumeration value="commissario"/>
									<xs:enumeration value="soggetto specializzato ex art. 107 comma 1 L.F."/>
									<xs:enumeration value="ufficiale giudiziario"/>
									<xs:enumeration value="altro"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Spese">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="ImposteTrasferimento" type="bt:Valuta" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Dato segretato</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Compensi" type="bt:Valuta">
							<xs:annotation>
								<xs:documentation>Dato segretato</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="CancellazioneFormalitaPregiudizievoli" type="bt:Valuta">
							<xs:annotation>
								<xs:documentation>Dato segretato</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Altre" type="bt:Valuta">
							<xs:annotation>
								<xs:documentation>Dato segretato</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="SpeseOccupazione" type="bt:Valuta">
							<xs:annotation>
								<xs:documentation>Dato segretato</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="StatoLotto" type="StatoLotto"/>
			<xs:element name="TipoLiquidazione">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="vendita competitiva"/>
						<xs:enumeration value="vendita secondo la disciplina del c.p.c."/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Note" type="xs:string" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="DepositoAPC">
		<xs:annotation>
			<xs:documentation>Deposito atto interoperabilita' APC</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="pt:AttoProcedimento">
					<xs:sequence>
						<xs:element name="deposito">
							<xs:annotation>
								<xs:documentation>Tipologia dell'atto veicolato</xs:documentation>
								<xs:appinfo source="http://www.giustizia.it/gl/sgr"/>
							</xs:annotation>
							<xs:complexType>
								<xs:choice>
									<xs:element name="inventario" type="bt:RiferimentoAllegato">
										<xs:annotation>
											<xs:documentation>Inventario come definito in APC v1.8.x</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="elencoInsinuazioni" type="bt:RiferimentoAllegato">
										<xs:annotation>
											<xs:documentation>Elenco delle insinuazioni come definito in APC v1.8.x</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="statoPassivo" type="bt:RiferimentoAllegato">
										<xs:annotation>
											<xs:documentation>Stato Passivo come definito in APC v1.8.x</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="relazioneIniziale" type="bt:RiferimentoAllegato">
										<xs:annotation>
											<xs:documentation>Relazione iniziale come definita in APC v1.8.x</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="relazionePeriodica" type="bt:RiferimentoAllegato">
										<xs:annotation>
											<xs:documentation>Relazione periodica come definita in APC v1.8.x</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:choice>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="DepositoSemplice">
		<xs:annotation>
			<xs:documentation>Deposito Atto semplice</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="pt:AttoProcedimento">
					<xs:sequence>
						<xs:element name="deposito">
							<xs:annotation>
								<xs:appinfo source="http://www.giustizia.it/gl/sgr"/>
								<xs:documentation>Specifica la tipologia di atto oggetto di deposito</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:choice>
									<xs:element ref="evt:depositoIstanzaFissazioneVenditaProgrammaLiquidazione"/>
									<xs:element ref="evt:attoNonCodificato"/>
								</xs:choice>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="DepositoRelazioneIniziale">
		<xs:annotation>
			<xs:documentation>Deposito relazione iniziale ex art. 33 L.F.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="pt:AttoProcedimento">
					<xs:choice minOccurs="0">
						<xs:element name="RelazioneStandard">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="ReclamoExArt18" type="xs:boolean">
										<xs:annotation>
											<xs:documentation>Presentato reclamo ex. art. 18</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="DatiBase">
										<xs:complexType>
											<xs:sequence>
												<xs:element name="DataCostituzione" type="xs:date"/>
												<xs:element name="Settore">
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:enumeration value="Industria"/>
															<xs:enumeration value="Commercio"/>
															<xs:enumeration value="Servizi"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
												<xs:element name="Mercato">
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:enumeration value="Locale"/>
															<xs:enumeration value="Nazionale"/>
															<xs:enumeration value="Estero"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
												<xs:element name="RicaviLordi">
													<xs:complexType>
														<xs:sequence maxOccurs="3">
															<xs:annotation>
																<xs:documentation>Anno in corso e i due precedenti</xs:documentation>
															</xs:annotation>
															<xs:element name="Anno" type="xs:positiveInteger"/>
															<xs:element name="Ricavo" type="bt:Valuta"/>
														</xs:sequence>
													</xs:complexType>
												</xs:element>
												<xs:element name="Dipendenti">
													<xs:complexType>
														<xs:sequence maxOccurs="3">
															<xs:annotation>
																<xs:documentation>Anno in corso e i due precedenti</xs:documentation>
															</xs:annotation>
															<xs:element name="Anno" type="xs:positiveInteger"/>
															<xs:element name="Numero" type="xs:positiveInteger"/>
														</xs:sequence>
													</xs:complexType>
												</xs:element>
												<xs:element name="Garanzie">
													<xs:complexType>
														<xs:sequence maxOccurs="unbounded">
															<xs:element name="Tipologia" type="xs:string"/>
															<xs:element name="Ammontare" type="bt:Valuta"/>
														</xs:sequence>
													</xs:complexType>
												</xs:element>
												<xs:element name="Attivo">
													<xs:complexType>
														<xs:sequence maxOccurs="3">
															<xs:annotation>
																<xs:documentation>Anno in corso e i due precedenti</xs:documentation>
															</xs:annotation>
															<xs:element name="Immobilizzato" type="bt:Valuta"/>
															<xs:element name="Immoblili" type="bt:Valuta"/>
															<xs:element name="Partecipazioni" type="bt:Valuta"/>
															<xs:element name="Circolante" type="bt:Valuta"/>
															<xs:element name="Crediti" type="bt:Valuta"/>
															<xs:element name="Magazzino" type="bt:Valuta"/>
															<xs:element name="Liquidita" type="bt:Valuta"/>
															<xs:element name="Altro" type="bt:Valuta"/>
														</xs:sequence>
													</xs:complexType>
												</xs:element>
												<xs:element name="Passivo">
													<xs:complexType>
														<xs:sequence maxOccurs="3">
															<xs:annotation>
																<xs:documentation>Anno in corso e i due precedenti</xs:documentation>
															</xs:annotation>
															<xs:element name="DebitiBreve" type="bt:Valuta">
																<xs:annotation>
																	<xs:documentation>Entro 12 mesi</xs:documentation>
																</xs:annotation>
															</xs:element>
															<xs:element name="DebitiLungo" type="bt:Valuta">
																<xs:annotation>
																	<xs:documentation>oltre 12 mesi</xs:documentation>
																</xs:annotation>
															</xs:element>
														</xs:sequence>
													</xs:complexType>
												</xs:element>
												<xs:element name="CapitaleProprio" type="bt:Valuta"/>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
									<xs:element name="Governance">
										<xs:complexType>
											<xs:sequence>
												<xs:element name="Modo">
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:enumeration value="Monistico"/>
															<xs:enumeration value="Dualistico"/>
															<xs:enumeration value="Tradizionale"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
												<xs:element name="Gruppo">
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:enumeration value="Partecipata"/>
															<xs:enumeration value="Holding"/>
															<xs:enumeration value="No"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
												<xs:element name="Soci">
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:enumeration value="Famigliare"/>
															<xs:enumeration value="Minore5Soci"/>
															<xs:enumeration value="Maggiore5Soci"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
												<xs:element name="Amministratore">
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:enumeration value="Unico"/>
															<xs:enumeration value="Consiglio"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
												<xs:element name="OrganoControllo">
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:enumeration value="Comitato"/>
															<xs:enumeration value="ComitatoRevisore"/>
															<xs:enumeration value="Revisore"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
												<xs:element name="AmministratoriNonSoci" type="xs:boolean"/>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
									<xs:element name="CauseDissesto" minOccurs="0">
										<xs:complexType>
											<xs:sequence>
												<xs:element name="Soggettive">
													<xs:complexType>
														<xs:sequence>
															<xs:element name="ErrateScelte" maxOccurs="unbounded">
																<xs:simpleType>
																	<xs:restriction base="xs:string">
																		<xs:enumeration value="Settore"/>
																		<xs:enumeration value="Prodotto"/>
																		<xs:enumeration value="Organizzazione"/>
																		<xs:enumeration value="Finanziarie"/>
																	</xs:restriction>
																</xs:simpleType>
															</xs:element>
