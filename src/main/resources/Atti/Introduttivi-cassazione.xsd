      <xs:complexContent>
        <xs:extension base="pt:AttoIntroduttivo">
          <xs:sequence>
            <xs:element ref="pt:AnagraficaProcedimento"/>
            <xs:element name="DomiciliazioneDifensore" minOccurs="0" maxOccurs="unbounded">
              <xs:annotation>
                <xs:documentation>Tipologia della domiciliazione difensore. E' possibile specificare combinazioni di tipologie per ogni difensore</xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:all>
                  <xs:element name="domiciliatario" minOccurs="0"/>
                  <xs:element name="difensore" minOccurs="0"/>
                  <xs:element name="pressoCorteCassazione" minOccurs="0"/>
                  <xs:element name="exArt135" minOccurs="0"/>
                </xs:all>
                <xs:attribute name="codiceFiscale" type="bt:CodiceFiscale">
                  <xs:annotation>
                    <xs:documentation>Codice fiscale del difensore iscritto</xs:documentation>
                  </xs:annotation>
                </xs:attribute>
              </xs:complexType>
              <xs:keyref name="FK_DIFENSORE_AVVOCATO" refer="pt:PK_AVVOCATO">
                <xs:selector xpath="."/>
                <xs:field xpath="@codiceFiscale"/>
              </xs:keyref>
            </xs:element>
            <xs:element name="RichiestaNotifica" type="xs:date" minOccurs="0">
              <xs:annotation>
                <xs:documentation>Data richiesta della notifica</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="Notifica" minOccurs="0">
              <xs:annotation>
                <xs:documentation>Data della notifica</xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:simpleContent>
                  <xs:extension base="xs:date">
                    <xs:attribute name="tipo" type="TipoNotifica"/>
                  </xs:extension>
                </xs:simpleContent>
              </xs:complexType>
            </xs:element>
            <xs:element name="InizioGiudizioPrimoGrado" minOccurs="0">
              <xs:complexType>
                <xs:all>
                  <xs:element name="anno" type="xs:string"/>
                  <xs:element name="autorita" type="xs:string"/>
                  <xs:element name="localita"/>
                </xs:all>
              </xs:complexType>
            </xs:element>
            <xs:element name="Materia" type="MaterieRicorso" minOccurs="0" maxOccurs="unbounded">
              <xs:annotation>
                <xs:documentation>Materie oggetto del ricorso</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="Provvedimento" minOccurs="0" maxOccurs="unbounded">
              <xs:annotation>
                <xs:documentation>Provvedimenti oggetto del ricorso</xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="Numero" type="xs:string"/>
                  <xs:element name="Luogo" type="xs:string"/>
                  <xs:element name="DataDeposito" type="xs:date"/>
                  <xs:element name="NRG" type="pt:RiferimentoFascicolo" minOccurs="0"/>
                  <xs:element name="DataNotifica" type="xs:date" minOccurs="0"/>
                  <xs:element name="DataComunicato" type="xs:date" minOccurs="0"/>
                </xs:sequence>
                <xs:attribute name="tipo" use="required">
                  <xs:annotation>
                    <xs:documentation>Tipologia del provvedimento</xs:documentation>
                  </xs:annotation>
                  <xs:simpleType>
                    <xs:restriction base="xs:string">
                      <xs:enumeration value="Definitivo">
                        <xs:annotation>
                          <xs:documentation>Provvedimento definitivo</xs:documentation>
                        </xs:annotation>
                      </xs:enumeration>
                      <xs:enumeration value="NonDefinitivo">
                        <xs:annotation>
                          <xs:documentation>Provvedimento non definitivo</xs:documentation>
                        </xs:annotation>
                      </xs:enumeration>
                      <xs:enumeration value="GiudizioPendente">
                        <xs:annotation>
                          <xs:documentation>Provvedimento con giudizio pendente</xs:documentation>
                        </xs:annotation>
                      </xs:enumeration>
                    </xs:restriction>
                  </xs:simpleType>
                </xs:attribute>
                <xs:attribute name="natura" use="required">
                  <xs:annotation>
                    <xs:documentation>Natura del provvedimento</xs:documentation>
                  </xs:annotation>
                  <xs:simpleType>
                    <xs:restriction base="xs:string">
                      <xs:enumeration value="PR">
                        <xs:annotation>
                          <xs:documentation>Provvedimento</xs:documentation>
                        </xs:annotation>
                      </xs:enumeration>
                      <xs:enumeration value="SE">
                        <xs:annotation>
                          <xs:documentation>Sentenza</xs:documentation>
                        </xs:annotation>
                      </xs:enumeration>
