                        </xs:annotation>
                      </xs:enumeration>
                      <xs:enumeration value="GP">
                        <xs:annotation>
                          <xs:documentation>Giudizio pendente</xs:documentation>
                        </xs:annotation>
                      </xs:enumeration>
                    </xs:restriction>
                  </xs:simpleType>
