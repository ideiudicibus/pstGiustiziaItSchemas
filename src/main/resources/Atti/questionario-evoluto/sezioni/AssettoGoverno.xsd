                              <xs:sequence>
                                <xs:element name="SocietaControllanteArt2359comma1n1" type="eSioNo"/>
                                <xs:element name="SocietaControllanteArt2359comma1n2" type="eSioNo"/>
                                <xs:element name="SocietaControllanteArt2359comma1n3" type="eSioNo"/>
