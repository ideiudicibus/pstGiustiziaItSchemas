														<xs:documentation>Informazioni bancarie</xs:documentation>
													</xs:annotation>
												</xs:enumeration>
												<xs:enumeration value="05">
													<xs:annotation>
														<xs:documentation>Credito principale</xs:documentation>
													</xs:annotation>
												</xs:enumeration>
												<xs:enumeration value="06">
